<h1>Index</h1>

<% @songs.each do |song| %>
    <%= link_to song_path(song) do %>
        <p><%= song.name%></p>
    <% end %>

    <%= link_to artist_path(song.artist) do %>
        <h1> <%= song.artist.name %></h1>  
    <% end %>

    <%= link_to genre_path(song.genre) do %>
        <h1><%= song.genre.name%></h1>
    <% end %>
<% end %>