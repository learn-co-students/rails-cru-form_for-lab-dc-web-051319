<h1>Songs#edit</h1>
<p>Find me in app/views/songs/edit.html.erb</p>


<h1>Edit Song Form </h1>
<%= form_for(@song) do |f| %>
<label> Song Name </label>
<%= f.text_field :name %>
<label> Artist ID </label>
<%= f.text_field :artist_id %>
<label> Genre ID </label>
<%= f.text_field :genre_id %>

<%= f.submit %>
<% end %>
